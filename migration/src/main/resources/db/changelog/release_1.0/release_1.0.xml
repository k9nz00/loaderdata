<?xml version="1.0" encoding="UTF-8"?>

<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.3.xsd
        http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/pro/liquibase-pro-4.3.xsd">

    <changeSet id="20211129_001" author="ASemka" dbms="postgresql">
        <preConditions>
            <sqlCheck expectedResult="0">SELECT count(*)
                                         FROM information_schema.schemata
                                         WHERE schema_name = 'loader'</sqlCheck>
        </preConditions>
        <sqlFile path="db/changelog/release_1.0/sql/0000.sql"/>
    </changeSet>
    <changeSet id="20211129_002" author="ASemka" dbms="postgresql">
        <preConditions>
            <sqlCheck expectedResult="0">SELECT count(*)
                                         FROM information_schema.columns
                                         where column_name = 'authorities'
                                           and data_type = 'ARRAY'
                                           and udt_schema = 'loader'
                                           and udt_name = '_authority'</sqlCheck>
        </preConditions>
        <sqlFile path="db/changelog/release_1.0/sql/0001.sql"/>
    </changeSet>
    <changeSet id="20211129_003" author="ASemka" dbms="postgresql">
        <preConditions>
            <not>
                <tableExists tableName="roles" schemaName="loader"/>
                <tableExists tableName="users" schemaName="loader"/>
            </not>
        </preConditions>
        <sqlFile path="db/changelog/release_1.0/sql/0002.sql"/>
    </changeSet>

    <changeSet id="20211214_001" author="ASemka" dbms="postgresql">
        <preConditions>
            <not>
                <indexExists schemaName="loader" tableName="users" columnNames="name" indexName="users_name_uindex"/>
            </not>
        </preConditions>
        <createIndex indexName="users_name_uindex" unique="true" schemaName="loader" tableName="users">
            <column name="name"/>
        </createIndex>
    </changeSet>

    <changeSet id="20220517_001" author="ASemka" dbms="postgresql">
        <preConditions>
            <not>
                <tableExists schemaName="loader" tableName="avatars"/>
            </not>
        </preConditions>

        <!-- последовательность стартует с значения "3", чтобы не было ошибки после установки дефолтных аватарок для предустановленых пользователей -->
        <createSequence
                schemaName="loader"
                sequenceName="avatars_id_seq"
                startValue="3"
                incrementBy="1"
                cycle="false"/>

        <createTable schemaName="loader" tableName="avatars">
            <column name="id" type="int" autoIncrement="false" defaultValueComputed="nextval('loader.avatars_id_seq')">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="user_id" type="int"/>
            <column name="filename" type="varchar"/>
            <column name="avatar_data" type="text"/>
            <column name="size" type="bigint"/>
            <column name="created_at" type="timestamp"/>
            <column name="updated_at" type="timestamp"/>
        </createTable>

        <insert tableName="avatars" schemaName="loader">
            <column name="id" type="int" value="1"/>
            <column name="user_id" type="int" value="1"/>
            <column name="filename" type="varchar" value="default_avatar"/>
            <column name="avatar_data" type="text"
                    value=""/>
            <column name="size" type="int" value="5840"/>
            <column name="created_at" type="timestamp" value="now()"/>
        </insert>

        <insert tableName="avatars" schemaName="loader">
            <column name="id" type="int" value="2"/>
            <column name="user_id" type="int" value="2"/>
            <column name="filename" type="varchar" value="default_avatar"/>
            <column name="avatar_data" type="text"
                    value=""/>
            <column name="size" type="int" value="4260"/>
            <column name="created_at" type="timestamp" value="now()"/>
        </insert>
    </changeSet>
</databaseChangeLog>
